class Solution {
public:
    void moveZeroes(vector<int>& nums) 
    {
     
       int a=0;
        
        for(int i=0;i<nums.size();i++)
        {
            
            if(nums[i]!=0)
            {
                 nums[a++] = nums[i];
             }
        }
        
        for(int j=a;j<nums.size();j++)
            nums[j]=0;
        
         
    }
};
