class Solution {
public:
    
    bool isValid(string s) 
    {
        stack<char> stack;
        string a;
        
        if( s[0] == ']' || s[0] == '}' || s[0] == ')' )
         return false;
        
        
        for( int i=0;i<s.length();i++)
        {
            if( s[i] =='(' || s[i] =='[' ||s[i] =='{' ) 
               stack.push(s[i]);
            else
            {
                if( stack.size()==0)
                    return false;
                if( s[i]!=')' && stack.top() == '(')
                    return false;
                if( s[i]!=']' && stack.top() == '[')
                    return false;
                if( s[i]!='}' && stack.top() == '{')
                    return false;
               stack.pop();      
            }
        }
        
       if(stack.size()!=0) 
              return false;
        else    
              return true;
       
        
    }
};
